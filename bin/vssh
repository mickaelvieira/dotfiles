#!/bin/bash

set -o nounset
set -o errexit
set -o pipefail
#set -x

if [[ -z "${PATH_VM}" ]]
then
    echo "The path of the current vagrant machine [PATH_VM] has not been assigned"
    exit
fi

cd "${PATH_VM}"

# SSH to vagrant machine with ElasticSearch port forwarding
vagrant ssh -- -R 9200:localhost:9200

